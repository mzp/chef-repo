;
; Netatalk 3.x configuration file
;

[Global]
log level = defalut:warn
log file = /var/log/netatalk.log
uam list = uams_clrtxt.so uams_dhx.so uams_dhx2.so uams_guest.so

[Time Machine]
path = /export/
time machine = yes
vol size limit = 262144

[Public]
path = /export/public/
file perm = 0666
directory perm = 0777
